<head itemscope itemtype="http://schema.org/WebSite">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta http-equiv="Cache-Control" content="no-transform" />
  <meta http-equiv="Cache-Control" content="no-siteapp" />
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <meta name="HandheldFriendly" content="True" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="author" content="<%= config.author %>">
  <meta name="copyright" content="Â© <%= date(Date.now(), 'YYYY') %> <%= config.author %>">
  <meta name="url" content="<%= config.url %>">
  <meta name="keywords" content="<%= keywords %>" />
  <% if (theme.facebook_sdk.enable && theme.facebook_sdk.webmaster) { %>
    <meta property="fb:admins" content="<%= theme.facebook_sdk.fb_admin %>" />
    <meta property="fb:app_id" content="<%= theme.facebook_sdk.app_id %>" />
  <% } %>
  <meta itemprop="name" content="<%= config.title %>"/>
  <meta itemprop="url" content="<%= config.url %>"/>
  <meta itemprop="creator" content="<%= config.author %>"/>
  <title><%= title %></title>
  <%- favicon_tag(config.favicon) %>
  <%- feed_tag(config.feed.path, { title: config.title }) %>
  <%- open_graph({
    twitter_id: theme.twitter,
    google_plus: theme.google_plus,
    fb_admins: theme.facebook_sdk.fb_admin, 
    fb_app_id: theme.facebook_sdk.app_id}) %>
  <% if(theme.canonical) { %>
    <link rel="canonical" href="<%= config.url %>/<%= page.canonical_path.replace("index.html", "") %>"/>
  <% } %>
  <link rel="alternate" hreflang="x-default" href="<%= config.url %>">
  <%- css("css/bundle.css") %>  
  <script>
    function setLoadingBarProgress(num) {
      document.getElementById("loading-bar").style.width=num+"%";
    };
  </script>
</head>